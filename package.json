{
  "name": "depth-first",
  "description": "Depth first search directed graphs",
  "version": "2.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/depth-first.git"
  },
  "scripts": {
    "prebuild:flow": "mkdirp dist",
    "build:flow": "flow-copy-source -v -i '**/*test.js' src dist",
    "build:js": "BABEL_ENV=node babel src -d dist --ignore '*test.js'",
    "build": "run-s build:*",
    "test:readme": "readme-assert -p --require babel-register --main ./src/index.js",
    "test:js": "jest",
    "test": "run-s test:*",
    "lint:eslint": "eslint src",
    "lint:flow": "flow",
    "lint": "run-s lint:*",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-env": "1.7.0",
    "eslint": "5.15.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "flow-bin": "0.94.0",
    "flow-copy-source": "2.0.3",
    "jest": "23.6.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.5",
    "readme-assert": "5.0.6",
    "rimraf": "2.6.3"
  },
  "keywords": [
    "depth first search",
    "dfs",
    "graphs",
    "graph",
    "dependencies",
    "directed graphs"
  ],
  "files": [
    "dist"
  ]
}
